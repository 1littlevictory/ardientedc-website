    <script type="text/javascript">
      $(document).ready(function() {
        $('a.buy').on('click', function(e) {
          e.preventDefault();
          if ($('.buttons').css('display') == 'none') {
            $('.buttons').slideDown();
          } else {
            $('.buttons').slideUp();
          }

          return false;
        })
        $('.close a').on('click', function(e) {
          e.preventDefault();
          $('.buttons').slideUp();
          return false;
        })
      })
